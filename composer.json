{
    "name": "the-shit/spotify",
    "description": "THE SHIT Spotify integration - Spotify's Here, It's Tight",
    "keywords": ["shit", "tight", "laravel-zero", "cli", "component", "spotify"],
    "homepage": "https://github.com/S-H-I-T/spotify",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/S-H-I-T/spotify/issues",
        "source": "https://github.com/S-H-I-T/spotify"
    },
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan@partridge.rocks"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel-zero/framework": "^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "suggest": {
        "jordanpartridge/github-client": "For GitHub integration components"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.2"
    },
    "autoload": {
        "psr-4": {
            "ConduitComponents\\Spotify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ConduitComponents\\Spotify\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["component"]
}
